{
  "name": "sample-commander",
  "version": "2.1.3",
  "description": "A spectrogram-assisted file browser & player for audio recordings",
  "author": "Lennart Pegel <SAMPLECOMMANDER@JUSTLEP.NET>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/justlep/sample-commander.git"
  },
  "issues": "https://github.com/justlep/sample-commander/issues",
  "keywords": [
    "audio",
    "sample",
    "recording",
    "spectrogram",
    "fieldrecording",
    "ffmpeg",
    "wave",
    "mp3",
    "player",
    "browser"
  ],
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder --publish never",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "jest --verbose"
  },
  "build": {
    "productName": "Sample Commander",
    "appId": "net.justlep.samplecommander",
    "copyright": "Copyright Â© 2019 Lennart Pegel",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/256x256.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/renderer/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "async": "^2.6.2",
    "buefy": "^0.8.18",
    "electron-store": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.3",
    "move-file": "^1.2.0",
    "readdirp": "^3.0.1",
    "typeface-cousine": "0.0.72",
    "typeface-roboto": "0.0.54",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vuex": "^3.1.1",
    "vuex-pathify": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "@types/jest": "^24.0.13",
    "ajv": "^6.10.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "babili-webpack-plugin": "^0.1.2",
    "bulma-slider": "^2.0.0",
    "cfonts": "^2.4.3",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^2.1.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^7.2.4",
    "electron-builder": "^20.40.2",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-multipane": "^0.9.5",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuedarkmode": "^0.3.23",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.1.3"
  }
}
